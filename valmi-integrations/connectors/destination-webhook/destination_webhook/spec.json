{
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Webhook",
    "type": "object",
    "required": [
      "url",
      "method"
    ],
    "additionalProperties": true,
    "properties": {
      "url": {
        "type": "string",
        "title": "URL",
        "description": "Webhook URL"
      },
      "method": {
        "type": "string",
        "enum": [
          "POST",
          "GET"
        ]
      }
    }
  }
}